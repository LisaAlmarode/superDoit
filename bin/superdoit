
gemstoneDir="`dirname $0`/../gemstone"

if [ "$GEMSTONE"x = "x" ] ; then
	export GEMSTONE="$gemstoneDir/gemstoneProductTree"
fi

$GEMSTONE/bin/topaz -l -- $1 \
	-I "$gemstoneDir/.topazini" \
	-C "GEM_SOLO_EXTENT=$gemstoneDir/gemstoneProductTree/../hidden/bin/extent0.rowan.dbf" \
	-q \
	-S gemstoneDir/superdoit.tpz
